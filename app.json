{
  "expo": {
    "name": "타로 일기",
    "slug": "TarotMind",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/app-icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-new.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "jsEngine": "jsc",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.TarotMind",
      "infoPlist": {
        "CFBundleDisplayName": "타로 일기"
      },
      "splash": {
        "image": "./assets/splash-new.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff"
      }
    },
    "android": {
      "package": "com.anonymous.TarotMind",
      "versionCode": 1,
      "permissions": [],
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#3100BB"
      },
      "icon": "./assets/app-icon.png",
      "splash": {
        "image": "./assets/splash-new.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "plugins": ["expo-mail-composer"]
  }
}
